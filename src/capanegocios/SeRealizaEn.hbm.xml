<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="capanegocios.SeRealizaEn" table="se_realiza_en">

<composite-id>
<key-property name="idlugar" column = "id_lugar" ></key-property>
<key-property name="iddeporte" column = "id_deporte" ></key-property>
</composite-id>

 <many-to-one name="lugar" class="capanegocios.Lugar" fetch="select" insert="false" update="false">
 <column name="id_lugar" not-null="true"/>
 </many-to-one>
 <many-to-one name="deporte" class="capanegocios.Deporte" fetch="select" insert="false" update="false">
 <column name="id_deporte" not-null="true"/>
 </many-to-one>

</class>
</hibernate-mapping>